<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.utu.studentcare.graphicmodeui.FXController">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem mnemonicParsing="false" text="New" />
            <MenuItem mnemonicParsing="false" text="Open…" />
            <Menu mnemonicParsing="false" text="Open Recent" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" onAction="#close" text="Close" />
            <MenuItem mnemonicParsing="false" text="Save" />
            <MenuItem mnemonicParsing="false" text="Save As…" />
            <MenuItem mnemonicParsing="false" text="Revert" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Preferences…" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Quit" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Edit">
          <items>
            <MenuItem mnemonicParsing="false" text="Undo" />
            <MenuItem mnemonicParsing="false" text="Redo" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Cut" />
            <MenuItem mnemonicParsing="false" text="Copy" />
            <MenuItem mnemonicParsing="false" text="Paste" />
            <MenuItem mnemonicParsing="false" text="Delete" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Select All" />
            <MenuItem mnemonicParsing="false" text="Unselect All" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" text="About MyHelloApp" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <Button fx:id="startTeachBtn" layoutX="218.0" layoutY="135.0" mnemonicParsing="false" onAction="#startTeach" prefHeight="26.0" prefWidth="160.0" text="Ala opettaa kurssia" />
            <Button fx:id="assessBtn" layoutX="218.0" layoutY="187.0" mnemonicParsing="false" onAction="#assess" prefHeight="26.0" prefWidth="160.0" text="Arvioi kurssisuorituksia" />
            <Button fx:id="evaluateBtn" layoutX="218.0" layoutY="243.0" mnemonicParsing="false" onAction="#writeRegister" prefHeight="26.0" prefWidth="160.0" text="Opintorekisteriin kirjaukset" />
            <Button fx:id="initializeDbBtn" layoutX="28.0" layoutY="321.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="126.0" text="Alusta tietokanta" />
            <Button fx:id="logOutBtn" layoutX="517.0" layoutY="321.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="96.0" text="Kirjaudu ulos" />
            <Button fx:id="startTeachBtn2" layoutX="218.0" layoutY="86.0" mnemonicParsing="false" onAction="#coursesAttended" prefHeight="26.0" prefWidth="160.0" text="Opiskelemasi kurssit" />
            <Button fx:id="joinCoursesBtn" layoutX="218.0" layoutY="38.0" mnemonicParsing="false" onAction="#joinCourses" prefHeight="26.0" prefWidth="160.0" text="Liity kurssille" />
        <Button fx:id="closeBtn" layoutX="218" layoutY="321.0" mnemonicParsing="false" onAction="#close" prefHeight="26.0" prefWidth="160.0" text="Sulje" />

      </children>
    </AnchorPane>
  </children>
</VBox>
